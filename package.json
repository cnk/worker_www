{
  "name": "worker-typescript-template",
  "version": "1.0.0",
  "description": "Cloudflare worker to replace the page rules for www.caltech.edu",
  "type": "module",
  "main": "./dist/index.mjs",
  "scripts": {
    "build": "node build.js",
    "dev": "miniflare --live-reload --debug",
    "format": "prettier --write  '*.{json,js}' 'src/**/*.{js,ts}' 'test/**/*.{js,ts}'",
    "lint": "eslint --max-warnings=0 src && prettier --check '*.{json,js}' 'src/**/*.{js,ts}' 'test/**/*.{js,ts}'",
    "test": "jest --config jest.config.js --verbose"
  },
  "author": "imss-ads-staff@caltech.edu",
  "license": "MIT OR Apache-2.0",
  "eslintConfig": {
    "root": true,
    "extends": [
      "typescript",
      "prettier"
    ]
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.1.1",
    "@types/jest": "^27.0.2",
    "@types/service-worker-mock": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "esbuild": "^0.13.13",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-config-typescript": "^3.0.0",
    "jest": "^27.3.1",
    "jest-environment-miniflare": "^2.0.0",
    "miniflare": "^2.0.0",
    "prettier": "^2.4.1",
    "service-worker-mock": "^2.0.5",
    "ts-jest": "^27.0.7",
    "ts-loader": "^9.2.2",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "itty-router": "^2.6.1"
  }
}